<div class="row">
    <div class="col-md-12">
        <h2>Posts</h2>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th>Date</th>
                    <th>Category</th>
                    <th>Published</th>
                </tr>
            </thead>
            <tbody>
                {{#each posts}}
                <tr>
                    <td>{{id}}</td>
                    <td>{{title}}</td>
                    <td>{{postDate}}</td>
                    <td>{{category}}</td>
                    <td>{{published}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
